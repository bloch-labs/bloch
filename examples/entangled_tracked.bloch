@quantum
function logic(qubit[] qreg) -> void {
    h(qreg[0]);
    cx(qreg[0], qreg[1]);
}


function main() -> void {
    // Track a 2-qubit register prepared in a Bell state.
    @tracked qubit[2] qreg;

    // Apply logic
    logic(qreg);
    
    // Explicitly measure to populate outcomes like 00/11
    measure qreg[0];
    measure qreg[1];
}


@quantum
function superposition() -> bit {
    qubit q;
    h(q);
    return measure q;
}

function main() -> void {
    int i = 0;
    int ones = 0;
    int zeros = 0;
    while (i < 10) {
        bit b = superposition();
        if (b == 1b) {
            ones++;
        } else {
            zeros++;
        }
        i = i + 1;
    }
echo("0s measured: " + zeros);
echo("1s measured: " + ones);
}
